{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(flutter *)",
      "Bash(dart *)",
      "Bash(git status *)",
      "Bash(git diff *)",
      "Bash(git log *)",
      "Bash(git branch *)",
      "Bash(git checkout *)",
      "Bash(git add *)",
      "Bash(git commit *)",
      "Bash(ls *)",
      "Bash(mkdir *)",
      "Bash(xcrun devicectl *)",
      "Bash(npx supabase *)",
      "Read(lib/**)",
      "Read(test/**)",
      "Read(assets/**)",
      "Edit(lib/**)",
      "Edit(test/**)",
      "Write(lib/**)",
      "Write(test/**)"
